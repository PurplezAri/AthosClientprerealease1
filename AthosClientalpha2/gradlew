#!/bin/sh
# Gradle start up script for POSIX systems.
# Minimal wrapper script stub (generated by Gradle wrapper normally).
# This file calls the Wrapper JAR to download and run Gradle.

DIR="$(cd "$(dirname "$0")" && pwd)"
JAVA_CMD="java"

if [ -n "$JAVA_HOME" ] ; then
  JAVA_CMD="$JAVA_HOME/bin/java"
fi

exec "$JAVA_CMD" -classpath "$DIR/gradle/wrapper/gradle-wrapper.jar" org.gradle.wrapper.GradleWrapperMain "$@"
